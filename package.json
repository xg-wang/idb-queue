{
  "name": "idb-time-series",
  "version": "0.1.0",
  "description": "A super-simple-small time-series store built on top of IndexedDB",
  "main": "./dist/cjs-compat/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./dist/*": "./dist/*/index.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/esm/index.d.ts",
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "PRODUCTION=true rollup -c && node lib/size-report.mjs",
    "dev": "rollup -cw & testem",
    "test": "rollup -c && testem ci",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg-wang/idb-timeseries.git"
  },
  "keywords": [
    "idb",
    "indexeddb",
    "store",
    "timeseries",
    "storage",
    "promise"
  ],
  "author": "Thomas Wang <hi@xgwang.me>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/xg-wang/idb-timeseries/issues"
  },
  "homepage": "https://github.com/xg-wang/idb-timeseries",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/runtime": "^7.14.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "del": "^6.0.0",
    "filesize": "^6.3.0",
    "glob": "^7.1.7",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "prettier": "^2.2.1",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^11.3.2",
    "testem": "^3.4.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,css,md,ts,html}": "prettier --write"
  },
  "volta": {
    "node": "16.1.0"
  }
}
